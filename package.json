{
  "name": "react-redux-scaffold",
  "description": "React Redux Scaffold",
  "version": "1.0.0",
  "author": "Tahmid Shakil",
  "repository": {
    "type": "git",
    "url": "https://github.com/atshakil/react-redux-scaffold.git"
  },
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dashboard -- webpack-dev-server --config ./config/webpack.config.dev.js  --hot --inline --progress",
    "start:prod": "http-server build -o",
    "start:prod:cold": "npm run clean:build && npm run build && http-server build -o",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "lint": "eslint config src/js/** --ext .js"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "2.8.0",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "faker": "3.1.0",
    "file-loader": "0.9.0",
    "jquery": "2.2.2",
    "jsdom": "9.6.0",
    "json-loader": "0.5.4",
    "node-sass": "3.10.1",
    "postcss-loader": "0.13.0",
    "react-hot-loader": "3.0.0-beta.5",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-slider-monitor": "1.0.7",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "autoprefixer": "6.5.0",
    "classnames": "2.2.5",
    "d3": "4.3.0",
    "fixed-data-table": "0.6.3",
    "http-server": "0.9.0",
    "lodash": "4.16.3",
    "react": "15.3.2",
    "react-bootstrap": "0.30.5",
    "react-contextmenu": "1.6.3",
    "react-dom": "15.3.2",
    "react-intl": "2.1.5",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-bootstrap": "0.23.1",
    "redux": "3.6.0",
    "redux-intl": "1.0.1"
  }
}
